generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model AiFeedback {
  id           Int      @id @default(autoincrement())
  submissionId String   @unique
  prompt       String
  response     Json
  source       String?
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
